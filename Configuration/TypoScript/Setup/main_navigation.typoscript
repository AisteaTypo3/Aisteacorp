lib.MAINMENU = HMENU
lib.MAINMENU {
    entryLevel = 0
    1 = TMENU
    1 {
        expAll = 1
        wrap = <nav><ul class="main-navigation">|</ul></nav>

        noBlur = 1
        NO = 1
        NO {
            ATagTitle.field = title
            wrapItemAndSub = <li>|</li>  
        }
        ACT < .NO
        ACT {
            wrapItemAndSub = <li class="active">|</li>
        }
    }
}